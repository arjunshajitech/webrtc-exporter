{
  "name": "webrtc-stats-exporter",
  "version": "1.0.2",
  "type": "module",
  "description": "WebRTC pc.getStats() collector with HTTP/gRPC export, batching and retry",
  "author": "Arjun Shaji <arjunshajitech@gmail.com>",
  "license": "MIT",
  "keywords": [
    "webrtc",
    "getstats",
    "observability",
    "metrics",
    "monitoring"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "rollup": "^4.59.0"
  }
}
